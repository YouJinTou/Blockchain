@model AddressHistoryViewModel

@{
    ViewData["Title"] = "Address";
}

<h2>@Model.Address</h2>

<h3>Balance: @Model.Balance</h3>

<div class="row form-group">
    <div class="col-xs-6">
        <h3>Incoming Transactions</h3>
        @Html.DisplayFor(m => m.InTransactions)
    </div>

    <div class="col-xs-6">
        <h3>Outgoing Transactions</h3>
        @Html.DisplayFor(m => m.OutTransactions)
    </div>
</div>
